<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pipikai.dao.TestDao">
    <!-- 目的：为DAO接口方法提供sql语句配置-->
    <select id="getTestObject" parameterType="Integer" resultType="com.pipikai.domain.TestObject">
        select * from test.test_object where id=#{id}
    </select>


</mapper>